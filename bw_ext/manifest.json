{
  "manifest_version": 2,
  "name": "BookWorm",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "bookworm.png",
    "default_popup": "html/credentials.html",
    "default_title": "Bookworm Login"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "contentSettings",
    "identity"
  ],
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://apis.google.com https://www.googleapis.com; object-src 'self'",
  "oauth2": {
    "client_id": "845803795351-r2hbl5s4lmoq6in62du7q35dqeol1qiv.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.openid"
    ]
  },
  "key": ""
}