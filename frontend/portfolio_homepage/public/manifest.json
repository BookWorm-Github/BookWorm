{

  "manifest_version": 2,
  "short_name": "BookWorm",
  "name": "BookWorm",

  "version": "1.0.1",
  "permissions": [
          "tabs","storage","alarms"
        ], 

  "background":{
    "scripts": ["background.js"],
    "persistent":false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
 
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
