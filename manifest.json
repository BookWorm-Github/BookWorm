{
  "manifest_version": 2,
  "name": "BookWorm",
  "version": "1.0",
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "css": ["collapsible.css"],
      "js":["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "bookworm.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
